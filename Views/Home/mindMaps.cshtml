@{
	Layout = "_Layout";
}

<body>
	@* location: { url: "../CONCEALED/mindMaps.pdf" } *@
	<div id="adobe-dc-view"></div>
	<script src="https://documentservices.adobe.com/view-sdk/viewer.js"></script>
	<script type="text/javascript">
		document.addEventListener("adobe_dc_view_sdk.ready", function () {
			var adobeDCView = new AdobeDC.View({ clientId: "72bf9e3792f746499129ba4f0ae522b6", divId: "adobe-dc-view" });
			adobeDCView.previewFile({
				content: { promise: "../CONCEALED/mindMaps.pdf" },
				metaData: { fileName: "mindMaps.pdf" },
				downloadWithCredentials: true
			},
				{
					defaultViewMode: "FIT_PAGE",
					enableFormFilling: false,
					showDownloadPDF: true,
					showPrintPDF: true,
					showAnnotationTools: true,
					showZoomControl: true,
					exitPDFViewerType: "CLOSE",
				}, {});
		});
	</script>
</body>

